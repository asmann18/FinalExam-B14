@model Department
<div class="card" style="width: 18rem;">
    <div class="card-body">
        <h5 class="card-title">Ddepartment Info</h5>
        <p class="card-text">@Model.Name</p>
        <h3>Employees:</h3>
        <ul>
            @foreach (var employee in Model.Employees)
            {
                <li>@employee.FullName</li>
            }
        </ul>
    </div>
</div>